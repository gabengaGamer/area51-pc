;
;   Pixel shader: kPS_PROJ_PRE_MULTIPLY
;
;==----------------------------------------------------------------------------

    mul r1.rgb,t0.rgb,v0.rgb +      ; R1.rgb = T0.rgb * V0.rgb
    mul r1.a  ,c7.a  ,t3.a          ; R1.a   = V0.a   * T3.a

    lrp r0.rgb,r1.a,t0.rgb,r1.rgb
